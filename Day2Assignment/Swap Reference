#include <stdio.h>

int swap (int *x, int *y)
{
    int temp;
    temp = *x;
    *x = *y;
    *y = temp;
}

void main()
{
    int a, b;
    printf("Enter value of a and b: ");
    scanf("%d%d",&a,&b);
    printf("\nBefore Swapping:\n");
    printf("\na = %d\nb = %d\n",a,b);
    swap(&a, &b);
    printf("\nAfter Swapping:\n");
    printf("\na = %d\nb = %d\n",a,b);
}
